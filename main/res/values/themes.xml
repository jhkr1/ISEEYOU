<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.ISEEYOU" parent="Theme.AppCompat.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_500</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>
    <style name="contrastTheme" parent="Theme.AppCompat.NoActionBar">
        <item name="textColor">@color/textColor</item>
        <item name="bgColor">@color/bgColor</item>
        <item name="setbtn">@drawable/set_c</item>
        <item name="backbtn">@drawable/back_c</item>
        <item name="capturebtn">@drawable/capture_c</item>
        <item name="detailebtn">@drawable/barcode_c</item>
        <item name="gallerybtn">@drawable/gallery_c</item>
        <item name="setmenu2">@drawable/setbtn2_c</item>
        <item name="settoggleoff">@drawable/settoggleoff_c</item>
        <item name="remove">@drawable/removebtn_c</item>
        <item name="serch">@drawable/serch_c</item>
        <item name="android:textViewStyle">@style/customTextViewFontStyle</item>
        <item name="android:buttonStyle">@style/customButtonFontStyle</item>
        <item name="android:editTextStyle">@style/customEditTextFontStyle</item>
        <item name="android:radioButtonStyle">@style/customRadioButtonFontStyle</item>
        <item name="android:checkboxStyle">@style/customCheckboxFontStyle</item>
    </style>
    <style name="notcontrastTheme" parent="Theme.AppCompat.NoActionBar">
        <item name="textColor">@color/black</item>
        <item name="bgColor">@color/white</item>
        <item name="setbtn">@drawable/set</item>
        <item name="backbtn">@drawable/back</item>
        <item name="capturebtn">@drawable/capture</item>
        <item name="detailebtn">@drawable/barcode</item>
        <item name="gallerybtn">@drawable/gallery</item>
        <item name="setmenu2">@drawable/setbtn2</item>
        <item name="settoggleoff">@drawable/settoggleoff</item>
        <item name="remove">@drawable/removebtn</item>
        <item name="serch">@drawable/serch</item>
        <item name="android:textViewStyle">@style/customTextViewFontStyle</item>
        <item name="android:buttonStyle">@style/customButtonFontStyle</item>
        <item name="android:editTextStyle">@style/customEditTextFontStyle</item>
        <item name="android:radioButtonStyle">@style/customRadioButtonFontStyle</item>
        <item name="android:checkboxStyle">@style/customCheckboxFontStyle</item>

    </style>


</resources>